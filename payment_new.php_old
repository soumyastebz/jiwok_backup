function normalze($string)	
{
	$fReplace2 	 = array('"','"','');
	$trans 	 = array('�','�','�');
	$trans2 = array_combine($trans, $fReplace2);
	$string = utf8_decode($string);
	return utf8_encode(strtr($string,$trans2));
}
